<div class="container">
  <h1>Contact Info</h1>
</div>

<div class="container">
  <% @contacts.each do |contact|%>
    <hr>
    <p><strong>Name:</strong> <a href="/contacts/<%= contact.id %>"><%= contact.full_name %></a></p>
    <p><strong>Email:</strong> <%= contact.email %></p>
    <p><strong>Phone Number:</strong> <%= contact.japan_phone %></p>
  <% end %>
</div>